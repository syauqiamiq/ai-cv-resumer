{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ai-cv-resumer/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ai-cv-resumer/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ai-cv-resumer",
  "projects": {
    "ai-cv-resumer": {
      "type": "application",
      "root": "apps/ai-cv-resumer",
      "entryFile": "main",
      "sourceRoot": "apps/ai-cv-resumer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ai-cv-resumer/tsconfig.app.json"
      }
    },
    "chroma": {
      "type": "library",
      "root": "libs/chroma",
      "entryFile": "index",
      "sourceRoot": "libs/chroma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/chroma/tsconfig.lib.json"
      }
    },
    "gemini": {
      "type": "library",
      "root": "libs/gemini",
      "entryFile": "index",
      "sourceRoot": "libs/gemini/src",
      "compilerOptions": {
        "tsConfigPath": "libs/gemini/tsconfig.lib.json"
      }
    },
    "kafka-mq": {
      "type": "library",
      "root": "libs/kafka-mq",
      "entryFile": "index",
      "sourceRoot": "libs/kafka-mq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka-mq/tsconfig.lib.json"
      }
    },
    "pdf": {
      "type": "library",
      "root": "libs/pdf",
      "entryFile": "index",
      "sourceRoot": "libs/pdf/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pdf/tsconfig.lib.json"
      }
    },
    "s3": {
      "type": "library",
      "root": "libs/s3",
      "entryFile": "index",
      "sourceRoot": "libs/s3/src",
      "compilerOptions": {
        "tsConfigPath": "libs/s3/tsconfig.lib.json"
      }
    },
    "worker": {
      "type": "application",
      "root": "apps/worker",
      "entryFile": "main",
      "sourceRoot": "apps/worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker/tsconfig.app.json"
      }
    }
  }
}